{
  "name": "certassist",
  "version": "1.0.0",
  "description": "JavaScript interface to ca.mit.edu and ca.csail.mit.edu",
  "homepage": "https://certassist.mit.edu/",
  "bugs": "https://github.com/sipb/certassist/issues",
  "license": "MIT",
  "author": "Anders Kaseorg",
  "repository": {
    "type": "git",
    "url": "https://github.com/sipb/certassist.git"
  },
  "scripts": {
    "build": "webpack -p",
    "watch": "webpack --mode=development --debug --output-pathinfo --watch"
  },
  "dependencies": {
    "@duosecurity/duo_web": "^1.0.4",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "core-js": "^3.0.1",
    "node-forge": "^0.8.3",
    "regenerator-runtime": "^0.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^1.0.1",
    "file-loader": "^1.1.11",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "raw-loader": "^0.5.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "corejs": 3,
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "strict": "error"
    },
    "overrides": [
      {
        "files": [
          "webpack.config.js"
        ],
        "env": {
          "browser": false,
          "es6": false,
          "node": true
        },
        "parserOptions": {
          "sourceType": "script"
        }
      }
    ]
  },
  "eslintIgnore": [
    "/dist"
  ],
  "prettier": {
    "trailingComma": "es5"
  }
}
