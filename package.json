{
  "name": "certassist",
  "version": "1.0.0",
  "description": "JavaScript interface to ca.mit.edu and ca.csail.mit.edu",
  "homepage": "https://certassist.mit.edu/",
  "bugs": "https://github.com/sipb/certassist/issues",
  "license": "MIT",
  "author": "Anders Kaseorg",
  "repository": {
    "type": "git",
    "url": "https://github.com/sipb/certassist.git"
  },
  "scripts": {
    "build": "webpack -p",
    "watch": "webpack --mode=development --debug --output-pathinfo --watch"
  },
  "dependencies": {
    "@duosecurity/duo_web": "^1.0.5",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "core-js": "^3.6.4",
    "node-forge": "^0.9.1",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@types/git-revision-webpack-plugin": "^3.0.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node-forge": "^0.9.2",
    "@types/webpack": "^4.41.7",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "file-loader": "^5.1.0",
    "git-revision-webpack-plugin": "^3.0.4",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "xo": "^0.27.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "corejs": 3,
          "useBuiltIns": "usage"
        }
      ],
      "@babel/typescript"
    ]
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false,
    "overrides": [
      {
        "files": "*.ejs",
        "options": {
          "parser": "html"
        }
      }
    ]
  },
  "xo": {
    "envs": [
      "browser",
      "es6"
    ],
    "prettier": true,
    "rules": {
      "strict": "error"
    },
    "overrides": [
      {
        "files": [
          "webpack.config.ts"
        ],
        "envs": [
          "node"
        ]
      }
    ]
  }
}
